{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/node-canvas",
      "dest": "/api/node-canvas.js"
    },
    {
      "src": "/napi-canvas",
      "dest": "/api/napi-canvas.js"
    },
    {
      "src": "/skia-canvas",
      "dest": "/api/skia-canvas.js"
    }
  ]
} 